@model List<PostListViewModel>

<div class="display_flex justify-content-between mb-20"> 
    <h4 class="text-left nav-text nav-box">文章管理</h4>
    @*<button id="btn_add" type="button" class="btn btn-primary" onclick="">新增分类</button>*@
</div>

<div class="text-center">
    <div class="table-responsive">
        <table class="table table-bordered table-hover" id="post_list">
            <thead class="thead-light">
                <tr>
                    <th class="wd_100">文章Id</th>
                    <th class="wd_100">文章标题</th>
                    <th class="wd_100">浏览量</th>
                    <th class="wd_100">发表时间</th>
                    <th class="wd_100">操作</th>
                </tr>
            </thead>
            <tbody>
                @Html.Partial("_PostListItem", Model)
            </tbody>
        </table>
    </div>
</div>